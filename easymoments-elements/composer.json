{
    "name": "typo3biswa/contao-easymoment",
    "description": "Simple Contao extension as a module and content element",
    "type": "contao-module",
    "homepage": "",
    "license": "LGPL-3.0-or-later",
    "authors":[
        {
            "name": "biswa",
            "homepage": "",
            "email": "onlinebiswajit@gmail.com",
            "role": "Developer"
        }
    ],
    "support":{
        "email": "onlinebiswajit@gmail.com",
        "issues": "",
        "source": "",
        "forum": "https://community.contao.org/de"
    },
    "require":{
        "php": ">=7.1",
        "contao/core-bundle": "^4.4 || ^5.0",
        "contao-community-alliance/composer-plugin": "^2.4 || ^3.0"
    },
    "replace": {
        "contao-legacy/sharebuttons": "self.version"
    },
    "extra":{
        "contao":{
            "sources":{
                "system/modules/sharebuttons": "system/modules/easyinfobox"
            }
        }
    },
    "autoload": {
        "classmap": [
            "system/modules/easyinfobox/elements/"
        ]
    }
}
